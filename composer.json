{
    "name": "moe-mizrak/laravel-log-reader",
    "description": "Lightweight Laravel package for reading, searching, and filtering logs from both file and database sources.",
    "keywords": [
        "Moe Mizrak",
        "laravel",
        "laravel-log-reader",
        "php 8"
    ],
    "type": "package",
    "homepage": "https://github.com/moe-mizrak/laravel-log-reader",
    "license": "MIT",
    "authors": [
        {
            "name": "Moe Mizrak"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-data": "^4.17"
    },
    "require-dev": {
        "orchestra/testbench": "^9.6.1",
        "phpunit/phpunit": "^11.4",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
          "MoeMizrak\\LaravelLogReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "MoeMizrak\\LaravelLogReader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "MoeMizrak\\LaravelLogReader\\LaravelLogReaderServiceProvider"
          ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
